{
    "include": [
        "python_benchmarks/"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__"
    ],
    "defineConstant": {
        "DEBUG": true
    },
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "reportUnboundVariable": false,
    "pythonVersion": "3.12",
    "pythonPlatform": "All",
    "executionEnvironments": [
        {
            "root": "python_benchmarks/",
            "pythonVersion": "3.12",
            "pythonPlatform": "All",
            "extraPaths": [
                "python_benchmarks/.venv/lib/python3.12/site-packages/"
            ]
        }
    ]
}
